{
  "name": "@qp/config",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "default": "./dist/env.js"
    },
    "./eslint": {
      "default": "./src/eslint/index.cjs"
    },
    "./prettier": {
      "default": "./src/prettier/index.cjs"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check ."
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rimraf": "^5.0.5",
    "typescript": "^5.9.3",
    "vitest": "^1.6.0"
  }
}
