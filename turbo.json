{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", ".next/**"],
      "cache": true
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": ["coverage/**", "reports/**"],
      "dependsOn": ["^build"],
      "cache": false
    },
    "typecheck": {
      "outputs": []
    },
    "format": {
      "cache": false
    }
  }
}
